{
  "name": "electron-react-seed",
  "productName": "electron-react-seed",
  "version": "1.0.0",
  "description": "My Electron react boilerplate",
  "main": "dist/main.prod.js",
  "scripts": {
    "dev": "cross-env START_HOT=1 npm run start-renderer-dev",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./src/main.dev",
    "start-renderer-dev": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.renderer.dev.js",
    "prod": "electron -r babel-register ./dist/main.prod",
    "linter": "eslint --cache --color --ext .jsx,.js src",
    "prettier-all": "prettier --config .prettierrc --write 'src/**/*.jsx' --write 'src/**/*.js'",
    "prettier-changed": "git diff --name-only --diff-filter=ACMRTU HEAD | grep -e \".*\\.jsx$\" -e \".*\\.js$\" | xargs prettier --config .prettierrc --write",
    "test": "mocha-webpack --require babel-register --require babel-polyfill --require init.test.js --webpack-config webpack.config.renderer.test.babel.js $(find src -type f  -name \"*spec.jsx\" -o -name \"*spec.js\" )",
    "precommit": "lint-staged && npm run linter && npm test",
    "package": "npm run build && build --publish never",
    "package-all": "npm run build && build -mwl",
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer\"",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.main.prod.js --colors",
    "build-renderer": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.prod.js --colors"
  },
  "keywords": [],
  "author": "krzysztofsztompka <sztompka.krzysztof@gmail,com>",
  "license": "MIT",
  "build": {
    "productName": "ElectronReact",
    "appId": "org.develar.ElectronReact",
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "main.prod.js",
      "main.prod.js.map",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.5",
    "compass-mixins": "^0.12.10",
    "electron-debug": "^1.5.0",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "i18next": "^10.5.1",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0-beta.13",
    "react-i18next": "^7.5.0",
    "react-notification-system": "^0.2.17",
    "react-notification-system-redux": "^1.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-observable": "^0.18.0",
    "reselect": "^3.0.1",
    "rxjs": "^5.5.6",
    "source-map-support": "^0.5.3",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-flow-runtime": "^0.17.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-shallow-deep-equal": "^1.4.6",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "dirty-chai": "^2.0.1",
    "electron": "^1.7.11",
    "electron-builder": "^20.5.1",
    "electron-devtools-installer": "^2.2.3",
    "electron-rebuild": "^1.7.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^15",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.7",
    "html-webpack-plugin": "^3.0.6",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "lint-staged": "^6.1.0",
    "mocha": "^4.1.0",
    "mocha-webpack": "^1.0.1",
    "node-sass": "^4.7.2",
    "plato": "^1.7.0",
    "prettier": "^1.10.2",
    "sass-loader": "^6.0.6",
    "sinon": "^4.4.2",
    "sinon-chai": "^2.14.0",
    "style-loader": "^0.20.2",
    "supertest": "^3.0.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "lint-staged": {
    "*.js": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  }
}
